        "dead":     { "transitions": [{"default":   "q.is_alive" }],    "on_entry": ["/function x/entity/common/health/on.death"],    "on_exit": ["/function x/entity/common/health/on.respawn"] }
    }},

    "controller.animation.common.is.using":             { "states": { "default": {"on_entry": ["/tag @s remove x.is.using"         ], "transitions": [{"is_using_item":"(query.is_using_item)"                                   }]}, "is_using_item":            {"on_entry": ["/tag @s add x.is.using"         ], "transitions": [{"default":"!(query.is_using_item)"                             }]} }},
